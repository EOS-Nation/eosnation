{
  "name": "eosnation",
  "version": "0.1.0",
  "description": "The Carmel EOSNation Product",
  "scripts": {
    "test": "react-savor test",
    "lint": "react-savor lint",
    "coverage": "react-savor coverage",
    "codeclimate": "react-savor codeclimate",
    "deploy:web": "aws s3 sync .chunky/web s3://eosnation.carmel.io --profile carmel",
    "package:web": "chunky package web && cp bp.json .chunky/web/bp.json",
    "push:web": "npm run package:web && npm run deploy:web",
    "push": "npm run push:web",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fluidtrends/eosnation.git"
  },
  "homepage": "http://eosnation.carmel.io",
  "dependencies": {
    "antd": "^3.12.1",
    "font-awesome": "^4.7.0",
    "raw-loader": "^0.5.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-dom-chunky": "^1.0.5",
    "react-reveal": "^1.2.2",
    "react-slick": "^0.23.2",
    "react-spring": "^5.5.1",
    "react-truncate": "^2.4.0",
    "react-twitter-embed": "^2.0.4",
    "react-vivus": "0.0.2",
    "styled-components": "^4.0.3",
    "twitter": "^1.7.1",
    "vivus": "^0.4.4"
  },
  "devDependencies": {
    "chunky-cli": "1.0.0-alpha.3",
    "react-savor": "0.x"
  }
}
