{
  "name": "eosnation",
  "version": "0.1.0",
  "description": "The Carmel EOSNation Product",
  "scripts": {
    "test": "react-savor test",
    "lint": "react-savor lint",
    "coverage": "react-savor coverage",
    "codeclimate": "react-savor codeclimate",
    "deploy:web": "aws s3 sync .chunky/web s3://eosnation.carmel.io --profile carmel",
    "package:web": "chunky package web",
    "push:web": "npm run package:web && npm run deploy:web",
    "push": "npm run push:web",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fluidtrends/eosnation.git"
  },
  "homepage": "http://eosnation.carmel.io",
  "dependencies": {
    "react-dom-chunky": "0.9.x",
    "font-awesome": "^4.7.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-spring": "^5.5.1",
    "react-truncate": "^2.4.0",
    "react-vivus": "0.0.2",
    "styled-components": "^4.0.3",
    "vivus": "^0.4.4"
  },
  "devDependencies": {
    "react-savor": "0.x"
  }
}
