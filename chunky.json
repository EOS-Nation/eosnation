{
  "name": "eosnation",
  "env": "dev",
  "id": "io.chunky",
  "androidSdkDir": "~/Library/Android/sdk",
  "sections": {
    "start": {
      "stack": ["intro"]
    }
  },
  "transitions": ["replace://start"],
  "provisioning": {},
  "theme": {
    "logoImage": "header-logo.png",
    "logoLightImage": "header-logo.png",
    "headerColor": "#FF5722",
    "textColor": "#546E7A",
    "linkColor": "#0288D1",
    "linkHoverColor": "#64B5F6",
    "linkHoverBackgroundColor": "#F5F5F5",
    "progressColor": "rgba(50,50,50,0.9)",
    "primaryColor": "#0097A7",
    "secondaryColor": "#D66C44",
    "statusBarLight": false,
    "navigationColor": "#2c4858",
    "stickyNavigation": true,
    "navigationTintColor": "#fff",
    "backgroundColor": "#999999",
    "footerTintColor": "#CFD8DC",
    "footerHeaderColor": "#90A4AE",
    "footerColor": "#546E7A",
    "footerBottomColor": "#37474F",
    "navigationWrapperStyle": {
      "padding": "15px"
    },
    "navigationButtonStyle": {
      "fontSize": "16px",
      "marginRight": "30px"
    },
    "navigationActionButtonStyle": {
      "height": "60px",
      "width": "240px",
      "fontSize": "16px"
    }
  },
  "info": {
    "copyright": "2018",
    "watermark": "Created with Carmel",
    "title": "EOS Nation"
  },
  "template": "personal",
  "additionalScripts": [
    {
      "rel": "stylesheet",
      "href": "https://use.fontawesome.com/releases/v5.5.0/css/all.css",
      "integrity": "ha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU",
      "crossOrigin": "anonymous"
    }
  ]
}
